#include<stdio.h>

void printASCII(char str[], int arr[])
{
    int i;

    for(i=0; str[i]!='\0'; i++)
    {
        arr[i]=str[i];
    }


}

int main()
{
    int i,arr[1000];
    char str[1000];

    printf("Enter any String : ");
    gets(str);
    printASCII(str,arr);

    printf("\nYour ASCII number is : ");
    for(i=0; str[i]!='\0'; i++)
    {
        printf("%d ",arr[i]);
    }

    return 0;
}
